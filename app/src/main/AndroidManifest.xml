<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shoptopadmin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShopTopAdmin">
        <activity
            android:name=".MarketForm"
            android:theme="@style/Theme.ShopTopAdmin.NoActionBar.TransparentStatusBar"/>
        <activity
            android:name=".Suppliers"
            android:theme="@style/Theme.ShopTopAdmin.NoActionBar.TransparentStatusBar"/>
        <activity
            android:name=".RegisterMarkets"
            android:theme="@style/Theme.ShopTopAdmin.NoActionBar.TransparentStatusBar"/>
        <activity
            android:name=".Users"
            android:theme="@style/Theme.ShopTopAdmin.NoActionBar.TransparentStatusBar"/>
        <activity
            android:name=".Notification"
            android:theme="@style/Theme.ShopTopAdmin.NoActionBar.TransparentStatusBar"/>
        <activity
            android:name=".Dashboard"
            android:theme="@style/Theme.ShopTopAdmin.NoActionBar.TransparentStatusBar"/>
        <activity
            android:name=".login"
            android:theme="@style/Theme.ShopTopAdmin.NoActionBar.TransparentStatusBar"/>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.ShopTopAdmin.NoActionBar.TransparentStatusBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>